{
  "name": "be-code-challenge",
  "version": "1.0.0",
  "description": "GAN Integrity backend code challenge",
  "main": "index.js",
  "type": "module",
  "node": "~20",
  "scripts": {
    "start": "nodemon --experimental-vm-modules index.js | npx pino-pretty",
    "test": "nodemon --test test/",
    "test:watch": "nodemon --watch --test test/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx,ts,tsx}": [
      "eslint --quiet --fix"
    ],
    "*.{json,js,ts,jsx,tsx,html}": [
      "prettier --write --ignore-unknown"
    ]
  },
  "author": "GAN Integrity",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fs-extra": "11.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "nodemon": "^3.0.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.3"
  }
}
