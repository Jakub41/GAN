{
  "plugins": ["prettier", "import"],
  "extends": ["plugin:prettier/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-debugger": "off",
    "no-console": 0
  }
}
